import pandas as pd

data_pandas_1 = {"Nama": ["Ali", "Budi", "Citra", "Dina"], "Usia": [34, 23, 29, 45]}
df_pandas_1 = pd.DataFrame(data_pandas_1)

data_pandas_2 = {"Nama": ["Ali", "Budi", "Citra", "Dina"], "Pekerjaan": ["Petani", "Desainer", "Dokter", "Guru"]}
df_pandas_2 = pd.DataFrame(data_pandas_2)

df_joined = pd.merge(df_pandas_1, df_pandas_2, on="Nama")
print(df_joined)

print(df_pandas_1.describe())

import matplotlib.pyplot as plt
df_pandas_1['Usia'].plot(kind='bar')
plt.show()
